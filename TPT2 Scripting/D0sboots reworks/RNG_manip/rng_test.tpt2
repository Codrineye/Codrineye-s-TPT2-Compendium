:import RNG_manip lib
:budget_cap 100
;
:local int predicted_rng
:local int actual_rng

:local string target

:global int count

key.t()

{rng.line_1}
{rng.line_2(guess)}

guess:
{rng.next}
predicted_rng = {rng.get_range.int(0, 2147483646)}
actual_rng = rnd(0, 2147483646)

count += 1
target = if(actual_rng == predicted_rng, "true", "false")
global.int.set(target, global.int.get(target) + 1)
gotoif(guess, count < 100000)